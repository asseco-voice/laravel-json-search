{
    "name": "asseco-voice/laravel-json-search",
    "description": "Laravel JSON search",
    "keywords": [
        "php",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "asseco-voice/laravel-json-query-builder": "^2.0",
        "laravel/framework": "^10.0",
        "asseco-voice/laravel-common": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^8.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asseco\\JsonSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\JsonSearch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\JsonSearch\\SearchServiceProvider"
            ]
        }
    }
}
